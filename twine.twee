    "Gale": [3, 5, 7, 8, 10, 12],
    "Inferno": [2, 5, 6, 8, 9, 11],
    "Drench": [4, 6, 7, 8, 11, 12],
    "Crystal": [1, 5, 6, 9, 10, 11]
}>>


<<set _finalMonster = $playerchoices.finalmonster>>
<<set _optimalForMonster = _optimalActions[_finalMonster]>>
<<set _bonusScore = 0>>

<<print "includes:">>
<<print $playerchoices.chosenactions>>

<<for _i to 0; _i lt 5; _i++>>
    <<if _optimalForMonster.includes($playerchoices.chosenactions[_i])>>
        <<print "includes:" + $playerchoices.chosenactions[_i]>>
        <<set _bonusScore += 0.8>>
    <</if>>
<</for>>

<<set _totalScore = _baseScore + _bonusScore>>
<<set _percentage = Math.round((_totalScore / 10) * 100)>>
<<set $playerchoices.score = _totalScore>>
<<set $playerchoices.percentage = _percentage>>





